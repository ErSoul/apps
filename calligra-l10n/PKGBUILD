build() {
  cd $srcdir
  #langs="bs ca ca@valencia da de el en_GB es et fi fr gl hu it kk nb nds nl pl pt pt_BR ru sk sl sv uk zh_CN zh_TW" 
  langs="bs ca ca@valencia cs da de el en_GB es et fi fr gl hu it ja kk nb nl pl pt pt_BR ru sk sv uk zh_CN zh_TW"
  for lang in $langs; do
    install -d build/${lang}
    pushd build/${lang}
      cmake ${srcdir}/calligra-l10n-${lang}-${pkgver} \
	-DCMAKE_BUILD_TYPE=Release \
	-DCMAKE_INSTALL_PREFIX=/usr
      make 
    popd
  done
}

package_calligra-l10n-bs() {
        pkgdesc=('Bosnian Localization for calligra')
        groups=('localization' 'calligra-l10n')
        cd $srcdir/build/bs
        make DESTDIR=$pkgdir install
}

