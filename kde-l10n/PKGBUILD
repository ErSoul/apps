	pkgdesc="KDE language support: Upper Sorbian"
	groups=('kde-uninstall' 'kde-l10n' 'localization')


	pushd ${srcdir}/kde-l10n-hsb-${pkgver}
		make DESTDIR=${pkgdir} install
	popd
}

