	pkgdesc="KDE language support: Hungarian"
	groups=('kde-uninstall' 'kde-l10n' 'localization')


	pushd ${srcdir}/kde-l10n-hu-${pkgver}
		make DESTDIR=${pkgdir} install
	popd
}

