	pkgdesc="KDE language support: Armenian"
	groups=('kde-uninstall' 'kde-l10n' 'localization')


	pushd ${srcdir}/kde-l10n-hy-${pkgver}
		make DESTDIR=${pkgdir} install
	popd
}

