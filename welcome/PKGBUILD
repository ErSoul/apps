
pkgname=welcome
pkgver=1.0
pkgrel=1
pkgdesc="Live Welcome app for KaOS"
arch=('x86_64')
url="http://kaosx.us/"
license=('GPL')
depends=('qt')
source=("welcome.tar.xz")
md5sums=('7bcfcea7a65687da05b117dc94eac1e8')

build() {
    cd "${srcdir}/welcome/"
   
    #/usr/lib/qt5/bin/qmake 
    qmake
    make clean
    make
}

package() {
    cd "${srcdir}/welcome/"
   
    make INSTALL_ROOT="$pkgdir/" install
}
