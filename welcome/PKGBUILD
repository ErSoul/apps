
pkgname=welcome
pkgver=3.12
pkgrel=1
pkgdesc="Live Welcome app for KaOS"
arch=('x86_64')
url="http://kaosx.us/"
license=('GPL')
depends=('qt5-base')
makedepends=('git')
source=("git://github.com/KaOSx/welcome.git")
md5sums=('SKIP')

build() {
    cd welcome/
   
    /usr/lib/qt5/bin/qmake welcome.pro
    make clean
    make
}

package() {
    cd welcome/
   
    make INSTALL_ROOT=${pkgdir}/ install
}
