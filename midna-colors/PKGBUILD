
pkgname=midna-colors
pkgver=2.0.2
pkgrel=1
pkgdesc="Color scheme for KaOS plasma theme Midna"
arch=('x86_64')
url=("http://kaosx.us")
license=('Artistic 2.0')
source=('Midna.colors')
md5sums=('1ec2a13f37cc44b4fa7eb904287de882')

package() {
  install -dm 755  $pkgdir/usr/share/color-schemes
  install -Dm 644 Midna.colors $pkgdir/usr/share/color-schemes/Midna.colors
}
