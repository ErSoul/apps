
pkgname=KaOS_wallpapers
pkgver=2.1
pkgrel=1
pkgdesc="KaOS Wallpaper Set"
arch=('x86_64')
url="http://kaosx.us"
license=('GPL3' 'LGPLv3' 'cc-by-nc-nd-3.0')
depends=('')
source=("http://sourceforge.net/projects/kaosx/files/sources/midna/wallpaper/${pkgname}-${pkgver}.tar.xz")
md5sums=('f152d7a536ac62d0f8f7a7dd45d7edec')

package() {
  cd ${srcdir}
  
  mkdir -p $pkgdir/usr/share/wallpapers
  cp -R KaOS_wallpapers/* $pkgdir/usr/share/wallpapers
}
